{
  "extends": [
    "react-app",
    "react-app/jest",
    "airbnb"
  ],
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "comma-dangle": [1],
    "linebreak-style": [0, "windows"],
    "no-unused-vars": [1],
    "react/prop-types": [1],
    "react/forbid-prop-types": [1]
  }
}